<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GameScene.MainOption.MainOptionController">
   <children>
      <VBox fx:id="MainOptionVBox" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" spacing="25.0">
         <children>
            <Button fx:id="StartGameButton" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onMouseClicked="#HandleStartGameButtom" prefWidth="100.0" text="Start Game" />
            <Button fx:id="BackButton" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onMouseClicked="#HandleBackButton" prefWidth="100.0" text="Back" />
            <Button fx:id="StartNewHandButton" layoutX="10.0" layoutY="35.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onMouseClicked="#HandleStartNewHandButton" prefWidth="100.0" text="StartNewHand" />
            <Button fx:id="ReplayButton" layoutX="10.0" layoutY="60.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onMouseClicked="#HandleReplayButton" prefWidth="100.0" text="Repaly" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
      <Separator />
      <VBox fx:id="VboxBuy" alignment="CENTER" spacing="10.0">
         <children>
            <Button fx:id="buyButtonPlayer1" mnemonicParsing="false" onAction="#HandleBuyPlayer1" text="Player 1 BUY" />
            <Button fx:id="buyButtonPlayer2" mnemonicParsing="false" onAction="#HandleBuyPlayer2" text="Player 2 BUY" />
            <Button fx:id="buyButtonPlayer3" mnemonicParsing="false" onAction="#HandleBuyPlayer3" text="Player 3 BUY" />
            <Button fx:id="buyButtonPlayer4" mnemonicParsing="false" onAction="#HandleBuyPlayer4" text="Player 4 BUY" />
            <Button fx:id="buyButtonPlayer5" mnemonicParsing="false" onAction="#HandleBuyPlayer5" text="Player 5 BUY" />
            <Button fx:id="buyButtonPlayer6" mnemonicParsing="false" onAction="#HandleBuyPlayer6" text="Player 6 BUY" />
         </children>
         <padding>
            <Insets bottom="10.0" top="10.0" />
         </padding>
      </VBox>
      <Separator />
      <VBox fx:id="VboxQuitGame" alignment="CENTER" spacing="10.0">
         <children>
            <Button fx:id="quitButtonPlayer1" mnemonicParsing="false" onAction="#HandleQuitPlayer1" text="Quit Player 1" />
            <Button fx:id="quitButtonPlayer2" mnemonicParsing="false" onAction="#HandleQuitPlayer2" text="Quit Player 2" />
            <Button fx:id="quitButtonPlayer3" mnemonicParsing="false" onAction="#HandleQuitPlayer3" text="Quit Playe 3" />
            <Button fx:id="quitButtonPlayer4" mnemonicParsing="false" onAction="#HandleQuitPlayer4" text="Quit Player 4" />
            <Button fx:id="quitButtonPlayer5" mnemonicParsing="false" onAction="#HandleQuitPlayer5" text="Quit Player 5" />
            <Button fx:id="quitButtonPlayer6" mnemonicParsing="false" onAction="#HandleQuitPlayer6" text="Quit Player 6 " />
         </children>
         <padding>
            <Insets bottom="10.0" top="10.0" />
         </padding>
      </VBox>
   </children>
</VBox>
